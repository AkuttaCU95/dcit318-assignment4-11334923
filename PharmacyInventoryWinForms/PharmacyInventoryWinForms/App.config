<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <connectionStrings>
    <!-- Replace Data Source with your SQL Server instance -->
    <add name="PharmacyDBConn" connectionString="Data Source=.;Initial Catalog=PharmacyDB;Integrated Security=True" />
  </connectionStrings>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
</configuration>